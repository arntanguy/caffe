# Training parameters for the siamese network
net: "imagenet_siamese.prototxt"
test_iter: 100
test_interval: 100
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0000
lr_policy: "inv"
gamma: 0.0001
power: 0.75
display: 10
stepsize: 100000
# The maximum number of iterations
max_iter: 100000
# Snapshot intermediate results
snapshot: 1000
snapshot_prefix: "siamese_trained"
solver_mode: GPU
